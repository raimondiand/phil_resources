@article{Rector2003,
abstract = {Modularity is a key requirement for large ontologies in order to achieve re-use, maintainability, and evolution. Mechanisms for 'normalisation' to achieve analogous aims are standard for databases. However, no similar notion of normalisation has yet emerged for ontologies. This paper proposes initial criteria for a two-step normalisation of ontologies implemented using OWL or related DL based formalisms. For the first - "ontological normalisation" - we accept Welty and Guarino's analysis. For the second - "implementation normalisation" - we propose an approach based on decomposing ("untangling") the ontology into independent disjoint skeleton taxonomies restricted to be simple trees, which can then be recombined using definitions and axioms to represent the relationships between them explicitly.},
author = {Rector, Alan L},
doi = {10.1145/945645.945664},
file = {:Users/raimondiand/Library/Application Support/Mendeley Desktop/Downloaded/Rector - 2003 - Modularisation of domain ontologies implemented in description logics and related formalisms including OWL.pdf:pdf},
isbn = {1581135831},
journal = {Proceedings of the international conference on Knowledge capture KCAP 03},
keywords = {description logics,ontologies,owl,semantic web},
pages = {121},
publisher = {ACM Press},
series = {K-CAP '03},
title = {{Modularisation of domain ontologies implemented in description logics and related formalisms including OWL}},
url = {http://portal.acm.org/citation.cfm?doid=945645.945664},
year = {2003}
}
